    "eject": "react-scripts eject",
    "emulate": "npm run build:functions && firebase emulators:start",
    "dev": "npx onchange -i -k \"functions/src/**.*\" -- npm run emulate",
    "deploy": "npm run build && firebase deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ts-standard": "^11.0.0"
  }
}
